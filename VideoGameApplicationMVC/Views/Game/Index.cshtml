@model List<GameViewModel>;
<div class="container">
  <h1 class="text-center display-4 light-text" style="margin-bottom:5%;margin-top:5%;">All Games</h1>
  <div class="d-flex flex-wrap justify-content-around align-items-around">
    @foreach (var game in Model)
    {
       @await Html.PartialAsync("_GameCard", game);
    }
  </div>
</div>